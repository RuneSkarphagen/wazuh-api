{
    "realms": {
        "native": {

            "user": {
                "/*": {
                    "methods": [
                        "GET", "PUT", "POST"
                    ],
                    "exceptions": [
                        "/security/users/*"
                    ]
                }
            },

            "viewer": {
                "/*": {
                    "methods": [
                        "GET"
                    ],
                    "exceptions": [
                        "/security/users*",
                        "/agents/*/key"
                    ]
                },
                "/api/user": {
                    "methods": [
                        "PUT"
                    ]
                }
            },


            "sample-group": {
                "/agents?status=active*": {
                    "methods": [
                        "GET"
                    ]
                },
                "/manager/*": {
                    "methods": [
                        "GET"
                    ]
                }
            }

        },
        "active_directory": {}
    }
}