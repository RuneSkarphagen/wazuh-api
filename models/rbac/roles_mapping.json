{
    "realms": {
        "native": {
            "admin": {
                "/agentss": {
                    "methods": [
                        "POST",
                        "PUT",
                        "DELETE"
                    ]
                }
            },
            "group1": {
                "/agents": {
                    "methods": [
                        "GET"
                    ]
                },
                "/manager": {
                    "methods": [
                        "PUT"
                    ]
                },
                "/syscollector*": {
                    "methods": [
                        "GET"
                    ]
                },
                "/api*": {
                    "methods": [
                        "GET",
                        "PUT",
                        "DELETE",
                        "POST"
                    ]
                },
                "/api/users/foo": {
                    "methods": [
                        "GET"
                    ]
                },
                "/manager/logs?pretty&limit=1&offset=1": {
                    "methods": [
                        "GET",
                        "PUT",
                        "DELETE",
                        "POST"
                    ]
                }
            },
            "viewer": {
                "/manager*": {
                    "methods": "*"
                },
                "/api*": {
                    "methods": [
                        "GET"
                    ]
                }
            },


            "user": {
                "/api/*": {
                    "methods": [
                        "GET", "POST", "PUT"
                    ],
                    "exceptions": [
                        "/api/users/foo"
                    ]
                }
            }
        },
        "active_directory": {}
    }
}